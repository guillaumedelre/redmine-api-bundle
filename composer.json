{
  "name": "guillaumedelre/redmine-api-bundle",
  "description": "symfony bundle handling redmine api.",
  "type": "bundle",
  "require": {
    "symfony/http-kernel": "^3.3",
    "symfony/dependency-injection": "^3.3",
    "symfony/framework-bundle": "^3.3",
    "csa/guzzle-bundle": "^2.2",
    "symfony/monolog-bundle": "^3.1",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "symfony/serializer": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Gdelre\\RedmineApiBundle\\": "src/",
      "Gdelre\\RedmineApiBundle\\App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdelre\\RedmineApiBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/console": "^3.3",
    "symfony/yaml": "^3.3"
  }
}
